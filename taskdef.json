{
  "executionRoleArn": "arn:aws:iam::476691507110:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "plx-dev",
      "image": "<IMAGE1_NAME>",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 80
        }
      ],
      "environmentFiles": [
        {
          "value": "arn:aws:s3:::plx-dev/test-aws.env",
          "type": "s3"
        }
      ],
      "pseudoTerminal": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "secretOptions": null,
        "options": {
          "awslogs-group": "/ecs/plx-dev",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "1024",
  "memory": "2048",
  "family": "plx-dev"
}
