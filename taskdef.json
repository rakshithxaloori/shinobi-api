{
  "executionRoleArn": "arn:aws:iam::476691507110:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "plx-dev",
      "image": "<IMAGE1_NAME>",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 80
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "1024",
  "memory": "2048",
  "family": "plx-dev"
}
