{
  "executionRoleArn": "<EXECUTION_ROLE_ARN>",
  "containerDefinitions": [
    {
      "name": "shn-lol-celery",
      "image": "<IMAGE_NAME>",
      "essential": true,
      "environmentFiles": [
        {
          "value": "arn:aws:s3:::shn/prod.env",
          "type": "s3"
        }
      ],
      "pseudoTerminal": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "secretOptions": [],
        "options": {
          "awslogs-group": "/ecs/shn-lol",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "secrets": []
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "1024",
  "memory": "2048",
  "family": "shn-lol"
}
