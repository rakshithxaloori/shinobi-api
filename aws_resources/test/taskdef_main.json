        },
        {
          "name": "RDS_USERNAME",
          "valueFrom": "arn:aws:ssm:us-east-1:476691507110:parameter/plx-dev/RDS_USERNAME"
        },
        {
          "name": "RDS_PASSWORD",
          "valueFrom": "arn:aws:ssm:us-east-1:476691507110:parameter/plx-dev/RDS_PASSWORD"
        },
        {
          "name": "RDS_HOSTNAME",
